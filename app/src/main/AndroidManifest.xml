<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.xavi.triaculturadroid">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_triacultura"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_triacultura"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity
            android:name=".LoginTriaCultura"
            android:screenOrientation="portrait"
            android:label="TriaCultura"
           >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TabbetsActivity"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_tabbets"
             />
        <activity android:name=".ViewProject"
            android:screenOrientation="portrait"/>
        <!-- <activity android:name=".Data.DialogEspera" /> -->
        <!-- <activity android:name=".DialogEspera" /> -->
        <activity android:name=".ProfileVotes"
            android:screenOrientation="portrait"/>
        <activity android:name=".ProjectActivityProfile"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>